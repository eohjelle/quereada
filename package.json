{
  "name": "quereada",
  "version": "0.2.0-alpha",
  "description": "A customizable way to engage with content on the internet.",
  "author": "Eivind Otto Hjelle",
  "private": true,
  "main": "./out/main/index.mjs",
  "scripts": {
    "db:build": "prisma generate && prisma migrate dev --name init",
    "electron:dev": "electron-vite dev --mode electron",
    "electron:build": "electron-vite build --mode electron",
    "electron:package:mac": "npm run electron:build && electron-builder --mac --config",
    "web:build": "vite build --emptyOutDir --mode web",
    "web:start": "tsx --inspect src/backend/main/web/index.ts",
    "test": "vitest"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/express": "^4.17.21",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.4.1",
    "@types/node-schedule": "^2.1.7",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "@types/xml2js": "^0.4.14",
    "electron": "^32.1.1",
    "electron-builder": "^25.1.8",
    "electron-vite": "^2.3.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.1",
    "util": "^0.12.5",
    "vite": "^5.4.5",
    "vitest": "^2.1.1"
  },
  "type": "module",
  "dependencies": {
    "@extractus/article-extractor": "^8.0.15",
    "@extractus/feed-extractor": "^7.1.3",
    "@fontsource-variable/eb-garamond": "^5.0.20",
    "@fontsource/baskervville-sc": "^5.0.2",
    "@fontsource/lugrasimo": "^5.0.6",
    "@prisma/client": "^5.18.0",
    "@sveltejs/adapter-node": "^5.2.2",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "electron-log": "^5.2.0",
    "esbuild": "^0.24.0",
    "esm": "^3.2.25",
    "express": "^4.21.0",
    "jsdom": "^25.0.1",
    "node-schedule": "^2.1.1",
    "openai": "^4.56.0",
    "prisma": "^5.18.0",
    "rss-parser": "^3.13.0",
    "svelte-inview": "^4.0.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "uuid": "^10.0.0",
    "ws": "^8.18.0",
    "xml2js": "^0.6.2",
    "zod": "^3.23.8"
  },
  "prisma": {
    "seed": "node ./database-seed.js"
  },
  "build": {
    "extraResources": [
      "./database-seed.js",
      "./migrations/**/*",
      "./schema.prisma",
      "./store.db",
      "./node_modules/.prisma/**/*",
      "./node_modules/@prisma/client/**/*",
      "./LICENSE"
    ]
  }
}
