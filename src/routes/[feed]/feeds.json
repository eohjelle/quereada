[
    {
        "feed_title": "Serious News",
        "blocks": [
            {
                "header": "Articles in The Atlantic",
                "prisma_function": "findMany",
                "prisma_args": {
                    "take": 10,
                    "where": {
                        "source_name": "The Atlantic"
                    },
                    "include": {
                        "authors": true
                    },
                    "orderBy": {
                        "date_published": "desc"
                    }
                }
            },
            {
                "header": "Articles in The New York Times",
                "prisma_function": "findMany",
                "prisma_args": {
                    "take": 10,
                    "where": {
                        "source_name": "The New York Times"
                    },
                    "include": {
                        "authors": true
                    },
                    "orderBy": {
                        "date_published": "desc"
                    }
                }
            }
        ]
    },
    {
